            },
            "dev": true,
            "servers": [],
            "envVariables": {},
            "links": []
          }
        ]
      }
    ],
    "defaultEnv": "default",
    "projects": [
      {
        "links": [],
        "name": "web-app-quickstart",
        "attributes": {
          "language": [
            "java"
          ]
        },
        "type": "blank",
        "source": {
          "location": "https://github.com/stour/web-app-quickstart.git",
          "type": "git",
          "parameters": {}
        },
        "path": "/web-app-quickstart",
        "problems": [],
        "mixins": [
          "git"
        ]
      }
    ],
    "name": "workspace",
    "commands": [
      {
        "commandLine": "([ -d $MW_HOME/user_projects/domains/base_domain ] && cd $MW_HOME/user_projects/domains/base_domain/bin && ./startWebLogic.sh) || (echo \\\"Domain base_domain does not exist.\\\")",
        "name": "Weblogic :: start",
        "attributes": {
          "previewUrl": "http://${server.port.7001}/console"
        },
        "type": "custom"
      },
      {
        "commandLine": "([ -d $MW_HOME/user_projects/domains/base_domain ] && cd $MW_HOME/user_projects/domains/base_domain/bin && ./stopWebLogic.sh) || (echo \\\"Domain base_domain does not exist.\\\")",
        "name": "Weblogic :: stop",
        "attributes": {
          "previewUrl": ""
        },
        "type": "custom"
      },
      {
        "commandLine": "([ -f ~/.gradle ] && [ -f ~/.gradle/gradle.properties ] || (mkdir ~/.gradle && echo systemProp.https.proxyHost=proxy.ops.tiaa-cref.org >> ~/.gradle/gradle.properties && echo systemProp.https.proxyPort=8080 >> ~/.gradle/gradle.properties)) && cd ${current.project.path} && gradle build",
        "name": "Project :: build",
        "attributes": {
          "previewUrl": ""
        },
        "type": "custom"
      },
      {
        "commandLine": "cd ${current.project.path} && gradle wlDeploy",
        "name": "Project :: deploy",
        "attributes": {
          "previewUrl": "http://${server.port.7001}/web-app-quickstart"
        },
        "type": "custom"
      },
      {
        "commandLine": "([ -f ~/.gradle ] && [ -f ~/.gradle/gradle.properties ] || (mkdir ~/.gradle && echo systemProp.https.proxyHost=proxy.ops.tiaa-cref.org >> ~/.gradle/gradle.properties && echo systemProp.https.proxyPort=8080 >> ~/.gradle/gradle.properties)) && cd ${current.project.path} && gradle build wlDeploy",
        "name": "Project :: build n deploy",
        "attributes": {
          "previewUrl": "http://${server.port.7001}/web-app-quickstart"
        },
        "type": "custom"
      }
    ],
    "links": []
  }
